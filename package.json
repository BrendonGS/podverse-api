{
  "name": "podverse-api",
  "version": "1.0.0",
  "description": "Data API, database migration scripts, and backend services for all Podverse models.",
  "main": "dist/server.js",
  "scripts": {
    "build": "webpack",
    "scripts:addAllFeedsToQueue": "ts-node -r tsconfig-paths/register ./src/scripts/queue/addAllFeedsToQueue.ts",
    "scripts:parseFeed": "ts-node -r tsconfig-paths/register ./src/scripts/parser/parseFeed.ts",
    "scripts:parseAllFeedsFromQueue": "ts-node -r tsconfig-paths/register ./src/scripts/parser/parseAllFeedsFromQueue.ts",
    "scripts:queryUniquePageviews": "ts-node -r tsconfig-paths/register ./src/scripts/stats/queryUniquePageviews.ts",
    "scripts:receiveNextFeedFromQueue": "ts-node -r tsconfig-paths/register ./src/scripts/queue/receiveNextFeedFromQueue.ts",
    "seeds:categories": "ts-node -r tsconfig-paths/register ./src/seeds/categories.ts",
    "seeds:podcasts": "ts-node -r tsconfig-paths/register ./src/seeds/podcasts.ts",
    "seeds:sampleEntities": "ts-node -r tsconfig-paths/register ./src/seeds/sampleEntities.ts",
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx,json --exec ts-node -r tsconfig-paths/register src/server.ts"
  },
  "license": "AGPLv3",
  "homepage": "https://github.com/podverse/podverse-api#readme",
  "keywords": [
    "podcast",
    "episode",
    "RSS",
    "Atom",
    "feed",
    "parser",
    "feedparser",
    "database",
    "db",
    "sequelize",
    "podverse"
  ],
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/dotenv": "^4.0.3",
    "@types/google.analytics": "^0.0.39",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-passport": "^4.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/koa__cors": "^2.2.3",
    "@types/node": "^10.9.4",
    "@types/passport": "^0.4.5",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/shelljs": "^0.8.0",
    "@types/validator": "^9.4.1",
    "koa2-swagger-ui": "^2.9.7",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "aws-sdk": "^2.315.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "date-fns": "^1.29.0",
    "googleapis": "^33.0.0",
    "http-errors": "^1.7.1",
    "joi": "^13.6.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.5.1",
    "koa-mount": "^3.0.0",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.9.0",
    "koa-static": "^5.0.0",
    "node-podcast-parser": "^2.3.0",
    "nodemailer": "^4.6.8",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.1",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "shortid": "^2.2.13",
    "tsconfig-paths": "^3.6.0",
    "typeorm": "^0.2.7",
    "uuid": "^3.3.2",
    "validator": "^10.7.1",
    "winston": "^3.1.0"
  }
}
